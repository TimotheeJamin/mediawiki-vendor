{
    "repositories": [
        {
            "type": "vcs",
            "url": "./.."
        }
    ],
    "require": {
        "wikimedia/composer-merge-plugin": "*@dev"
    },
    "config": {
        "preferred-install": "source"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "composer.local.json",
                "extensions/*/composer.json"
            ]
        }
    }
}
